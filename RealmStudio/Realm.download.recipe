<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Description</key>
        <string>Downloads the latest version of Realm dmg from Realm Studio.</string>
        <key>Identifier</key>
        <string>com.github.munki-tvlk.download.Realm</string>
        <key>MinimumVersion</key>
        <string>0.6.0</string>
        <key>Input</key>
        <dict>
            <key>NAME</key>
            <string>Realm</string>
        </dict>
        <key>Process</key>
        <array>
            <dict>
                <key>Processor</key>
                <string>URLDownloader</string>
                <key>Arguments</key>
                <dict>
                    <key>url</key>
                    <string>https://studio-releases.realm.io/latest/download/mac-dmg</string>
                    <key>filename</key>
                    <string>%NAME%.dmg</string>
                </dict>
            </dict>
            <dict>
                <key>Processor</key>
                <string>EndOfCheckPhase</string>
            </dict>
            <dict>
                <key>Processor</key>
                <string>CodeSignatureVerifier</string>
                <key>Arguments</key>
                <dict>
                    <key>input_path</key>
                    <string>/Volumes/Realm Studio 15.2.1/Realm Studio.app</string>
                    <key>requirement</key>
                    <string>identifier "io.realm.studio" and anchor apple generic and certificate leaf[subject.OU] = "4XWMY46275"</string>
                </dict>
            </dict>
        </array>
    </dict>
</plist>
